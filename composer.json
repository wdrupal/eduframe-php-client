{
  "name": "drieam/eduframe-php-client",
  "version": "1.3.0",
  "type": "library",
  "minimum-stability": "stable",
  "description": "A PHP Client for the Eduframe V1 API",
  "keywords": [
    "api",
    "php",
    "eduframe"
  ],
  "homepage": "https://github.com/Drieam/eduframe-php-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Luuk van Hulten",
      "email": "luuk@drieam.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phan/phan": "^5.4",
    "squizlabs/php_codesniffer": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "*"
  },
  "autoload": {
    "psr-4": {
      "Eduframe\\": "src/"
    }
  },
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
